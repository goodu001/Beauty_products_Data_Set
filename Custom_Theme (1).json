{
  "name": "Custom Theme",
  "dataColors": [
    "#F7B2BD",
    "#FBC4AB",
    "#FFD6A5",
    "#FDFFB6",
    "#CAFFBF",
    "#9BF6FF",
    "#A0C4FF",
    "#BDB2FF"
  ],
  "background": "#FFFFFF",
  "foreground": "#000000",
  "tableAccent": "#8B0000",
  "good": "#00B971",
  "neutral": "#000000",
  "bad": "#D6052E",
  "textClasses": {
    "label": {
      "color": "#000000"
    },
    "callout": {
      "color": "#8B0000"
    },
    "header": {
      "color": "#8B0000"
    }
  },
  "visualStyles": {
    "*": {
      "*": {
        "background": [
          {
            "color": {
              "solid": {
                "color": "#FFFFFF"
              }
            },
            "transparency": 0
          }
        ],
        "border": [
          {
            "color": {
              "solid": {
                "color": "#D8D8D8"
              }
            },
            "show": true,
            "radius": 5
          }
        ],
        "title": [
          {
            "color": {
              "solid": {
                "color": "#000000"
              }
            },
            "fontFamily": "Segoe UI, sans-serif"
          }
        ]
      }
    }
  }
}